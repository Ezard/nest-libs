{
  "name": "@apposing/nest-google-cloud-trace",
  "version": "1.2.2",
  "description": "A Nest.js wrapper around Google Cloud Trace, with a GraphQL plugin",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js"
  },
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.build-cjs.json",
    "build:esm": "tsc --project tsconfig.build-esm.json",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint src",
    "lint:style": "prettier --check src",
    "prepublishOnly": "npm run build",
    "test": "npm run test:unit",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apposing/apposing-nest-libs.git"
  },
  "author": "Ben Ezard",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/apposing/apposing-nest-libs/issues"
  },
  "homepage": "https://github.com/apposing/apposing-nest-libs/google-cloud-trace#readme",
  "dependencies": {
    "@google-cloud/opentelemetry-cloud-trace-exporter": "1.0.0",
    "@opentelemetry/sdk-trace-base": "1.0.1",
    "apollo-server-plugin-base": "3.5.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "9.1.2",
    "@opentelemetry/api": "^1.0.0"
  },
  "devDependencies": {
    "@nestjs/platform-express": "8.2.5",
    "@nestjs/testing": "8.2.5",
    "@types/jest": "27.4.0",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "apollo-server-express": "3.6.2",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "supertest": "6.2.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.4"
  }
}
