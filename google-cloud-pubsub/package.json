{
  "name": "@apposing/nest-google-cloud-pubsub",
  "version": "2.0.0",
  "description": "A Nest.js wrapper around Google Cloud Pub/Sub",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js"
  },
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.build-cjs.json",
    "build:esm": "tsc --project tsconfig.build-esm.json",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint src",
    "lint:style": "prettier --check src",
    "prepublishOnly": "npm run build",
    "test": "npm run test:unit",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apposing/apposing-nest-libs.git"
  },
  "author": "Ben Ezard",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/apposing/apposing-nest-libs/issues"
  },
  "homepage": "https://github.com/apposing/apposing-nest-libs/google-cloud-pubsub#readme",
  "peerDependencies": {
    "@google-cloud/pubsub": "^3.0.0",
    "@nestjs/common": "^8.0.0"
  },
  "devDependencies": {
    "@google-cloud/pubsub": "3.0.1",
    "@nestjs/common": "8.4.7",
    "@nestjs/testing": "8.4.7",
    "@types/jest": "28.1.1",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.1",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.5",
    "typescript": "4.7.3"
  }
}
